{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "TOKEN_SECRET": "@token_secret",
    "FRONTEND_URL": "@frontend_url"
  }
}
